{
  "name": "@akord/akord-cli",
  "version": "0.1.9",
  "description": "Akord Command Line Interface",
  "main": "lib/index.js",
  "repository": "git@github.com:Akord-com/akord-cli.git",
  "bin": {
    "akord": "lib/index.js"
  },
  "scripts": {
    "test": "jest --watchAll --verbose false --collectCoverage=false",
    "build": "tsc -p .",
    "local": "sudo npm install -g && akord",
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts"
  },
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "lib/index.d.ts"
  ],
  "keywords": [
    "akord",
    "cli",
    "vault",
    "storage",
    "collaboration",
    "arweave",
    "permaweb",
    "blockchain",
    "tooling",
    "web3"
  ],
  "bugs": {
    "url": "https://github.com/Akord-com/akord-cli/issues"
  },
  "homepage": "https://github.com/Akord-com/akord-cli#readme",
  "author": "Akord Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0 <17.0.0"
  },
  "dependencies": {
    "@akord/akord-js": "^0.3.53",
    "@akord/crypto": "^0.0.43",
    "amazon-cognito-identity-js": "^5.2.8",
    "cross-fetch": "^3.1.5",
    "figlet": "^1.5.2",
    "inquirer": "^8.2.0",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/node": "^7.17.10",
    "@babel/preset-env": "^7.12.1",
    "@types/figlet": "^1.5.4",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "jest": "^27.3.1",
    "nodemon": "^2.0.6",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.2.1",
    "typescript": "4.1.3"
  }
}
