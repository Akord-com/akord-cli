{
  "name": "@akord/akord-cli",
  "version": "1.2.1",
  "description": "A command-line interface to interact with Akord",
  "main": "lib/index.js",
  "repository": "git@github.com:Akord-com/akord-cli.git",
  "bin": {
    "akord": "lib/index.js"
  },
  "scripts": {
    "test": "jest --watchAll --verbose false --collectCoverage=false",
    "build": "tsc -p .",
    "local": "sudo npm install -g && akord",
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts"
  },
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "lib/index.d.ts"
  ],
  "keywords": [
    "akord",
    "cli",
    "vault",
    "storage",
    "collaboration",
    "arweave",
    "permaweb",
    "blockchain",
    "tooling",
    "web3"
  ],
  "bugs": {
    "url": "https://github.com/Akord-com/akord-cli/issues"
  },
  "homepage": "https://github.com/Akord-com/akord-cli#readme",
  "author": "Akord Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@akord/akord-js": "5.1.1",
    "@aws-sdk/client-s3": "^3.327.0",
    "@aws-sdk/lib-storage": "^3.327.0",
    "cli-color": "^2.0.3",
    "cli-progress": "^3.12.0",
    "cross-fetch": "^3.1.5",
    "figlet": "^1.5.2",
    "inquirer": "^8.2.0",
    "keytar": "^7.9.0",
    "redact-secrets": "^1.0.0",
    "winston": "^3.8.2",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@types/cli-color": "^2.0.2",
    "@types/cli-progress": "^3.11.0",
    "@types/figlet": "^1.5.4",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@types/redact-secrets": "^1.0.1",
    "@types/winston": "^2.4.4",
    "jest": "^27.3.1",
    "nodemon": "^2.0.6",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.2.1",
    "typescript": "4.1.3"
  }
}
